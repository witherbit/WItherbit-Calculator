        <TextBlock Text="(" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontFamily="Arial Black" FontSize="18" Margin="1 0"/>
        <StackPanel Orientation="Horizontal" x:Name="uiInput">

        </StackPanel>
        <TextBlock Text=")" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontFamily="Arial Black" FontSize="18" Margin="1 0"/>
    </StackPanel>
